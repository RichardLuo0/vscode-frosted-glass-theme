{
  "filter": {
    "default": "url(#fgt-acrylic-{key})",
    "minimap": "url(#fgt-mica)",
    "decorationsOverviewRuler": "url(#fgt-mica)",
    "terminalOverlay": "url(#fgt-mica)",
    "editorStickyScroll": {
      "filter": "url(#fgt-mica)",
      "disableBackgroundColor": false
    }
  },
  "transition": "300ms",
  "svg": [
    "../resource/mica.svg"
  ],
  "tintSvg": [
    "../resource/acrylic.svg"
  ],
  "variable": {
    "fgt-luminosity-opacity": "45%",
    "fgt-mica-luminosity": "hsl(0, 0%, 80%)",
    "fgt-mica-brightness": ""
  },
  "variableDark": {
    "fgt-luminosity-opacity": "85%",
    "fgt-mica-luminosity": "hsl(0, 0%, 20%)",
    "fgt-mica-brightness": "brightness(30%)"
  },
  "opacity": {
    "selection": 1,
    "border": 0.4,
    "separator": 0.7,
    "minimap": 0.4
  },
  "animation": {
    "menu": "300ms cubic-bezier(0, 0.8, 0.2, 1) 0s 1 forwards fgtDropdown",
    "dialog": "300ms cubic-bezier(0, 0.8, 0.2, 1) 0s 1 forwards fgtZoomIn"
  },
  "revealEffect": {
    "enabled": true,
    "opacity": 0.1,
    "gradientSize": 200,
    "clickEffect": true
  },
  "borderRadius": {
    "menu": "",
    "menuItem": "",
    "suggestWidget": ""
  },
  "fakeMica": {
    "enabled": false,
    "url": "C:/Windows/Web/Wallpaper/Windows/img0.jpg",
    "filter": "url(#fgt-mica) var(--fgt-mica-brightness)",
    "titlebarFix": true,
    "titlebarOpacity": 0.8,
    "listBackgroundFix": true,
    "editorBackgroundFix": true
  },
  "miscellaneous": {
    "progressBarBehindSectionHeader": false
  }
}